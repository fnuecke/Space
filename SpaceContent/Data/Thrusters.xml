<?xml version="1.0" encoding="utf-8" ?>
<XnaContent>
  <Asset Type="Space.ComponentSystem.Constraints.ThrusterConstraints[]">
    <!-- The type of thruster each player starts with, which has fixed
         values (enforced by min an max being equal). -->
    <Item>
      <Name>Starter Thruster</Name>
      <GuaranteedAttributes>
        <Item>2 AccelerationForce</Item>
        <Item>1 RotationForce</Item>
        <Item>3 ThrusterEnergyConsumption</Item>
      </GuaranteedAttributes>
    </Item>
    
    <!-- NPC thrusters -->
    <Item>
      <Name>Level 1 AI Thruster</Name>
      <GuaranteedAttributes>
        <Item>1.5 to 3 AccelerationForce</Item>
        <Item>0.75 to 1.5 RotationForce</Item>
        <Item>2 to 3 ThrusterEnergyConsumption</Item>
      </GuaranteedAttributes>
    </Item>
  </Asset>
</XnaContent>
