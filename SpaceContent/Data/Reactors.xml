      <MinEnergy>50</MinEnergy>
      <MaxEnergy>50</MaxEnergy>
      <MinEnergyRegeneration>5</MinEnergyRegeneration>
      <MaxEnergyRegeneration>5</MaxEnergyRegeneration>
    </Item>

    <!-- NPC reactors -->
    <Item>
      <Name>Level 1 AI Reactor</Name>
      <MinEnergy>50</MinEnergy>
      <MaxEnergy>75</MaxEnergy>
      <MinEnergyRegeneration>3</MinEnergyRegeneration>
      <MaxEnergyRegeneration>5</MaxEnergyRegeneration>
    </Item>
  </Asset>
</XnaContent>
