<?xml version="1.0" encoding="utf-8" ?>
<XnaContent>
  <Asset Type="Space.ComponentSystem.Constraints.ReactorConstraints[]">
    <!-- The type of reactor each player starts with, which has fixed
         values (enforced by min an max being equal). -->
    <Item>
      <Name>Starter Reactor</Name>
      <GuaranteedAttributes>
        <Item>50 Energy</Item>
        <Item>2.5 EnergyRegeneration</Item>
      </GuaranteedAttributes>
    </Item>

    <!-- NPC reactors -->
    <Item>
      <Name>Level 1 AI Reactor</Name>
      <GuaranteedAttributes>
        <Item>50 to 75 Energy</Item>
        <Item>2 to 3 EnergyRegeneration</Item>
      </GuaranteedAttributes>
    </Item>
  </Asset>
</XnaContent>
